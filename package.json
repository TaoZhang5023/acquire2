  "scripts": {
    "build": "yarn build-client ; yarn build-examples ; yarn build-review ; yarn build-server",
    "build-client": "APP=index MODE=production webpack",
    "build-examples": "APP=examples MODE=production webpack",
    "build-review": "APP=review MODE=production webpack",
    "build-server": "tsc --project tsconfig.server.json",
    "client": "APP=index yarn webpack-dev-server",
    "examples": "APP=examples yarn webpack-dev-server",
    "review": "APP=review yarn webpack-dev-server",
    "server": "nodemon --exec ts-node src/server/main.ts",
    "protobuf": "pbjs -t static-module -w commonjs -o src/common/pb.js src/common/pb.proto ; pbts -o src/common/pb.d.ts src/common/pb.js",
    "test": "jest",
    "lint": "eslint . --fix --ext .ts,.tsx",
    "format": "prettier --write '{,!(.git|node_modules)/**/}*.{css,js,json,md,scss,ts,tsx}' --loglevel warn"
  },
  "jest": {
    "transform": {
      "\\.tsx?$": "ts-jest"
    },
    "testRegex": "\\.test\\.tsx?$",
    "testURL": "http://localhost",
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ],
    "moduleNameMapper": {
      "\\.s?css$": "identity-obj-proxy"
    }
  },
  "prettier": {
    "printWidth": 160,
    "singleQuote": true,
    "trailingComma": "all"
  }
}
